<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace : xml파일은 여러개일 수 있으므로 이를 구별하기 위한 식별 용도로 사용 -->
<mapper namespace="mem">
	<!-- MEMBER 테이블에 insert -->
	<insert id="membInsert" parameterType="memVO">
		INSERT INTO MEM(MEM_ID,MEM_NAME,MEM_JOB,MEM_LIKE,MEM_SKILL)
		VALUES(#{memId},#{memName},#{memJob},#{memLike},#{memSkill})
	</insert>
	
	<select id="list" resultType="memVO" parameterType="String">
		SELECT MEM_ID, MEM_NAME, MEM_JOB, MEM_LIKE, MEM_SKILL
		FROM   MEM
		ORDER BY MEM_ID DESC
	</select>
	

</mapper>